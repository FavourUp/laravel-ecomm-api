{
    "name": "favourup/ecomm-api",
    "description": "A Laravel package that wraps the official APIs of multiple e-commerce web builders, such as WooCommerce/Wordpress and Shopify.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Wang",
            "email": "andre@favourup.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FavourUp\\EcommAPI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FavourUp\\EcommAPI\\EcommAPIServiceProvider"
            ],
            "aliases": {
                "EcommAPI": "FavourUp\\EcommAPI\\Facade"
            }
        }
    }
}
